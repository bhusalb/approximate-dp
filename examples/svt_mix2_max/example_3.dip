
INPUTSIZE 3;
RANDOM TH = gauss(eps/2, 0);
OUTPUT = [0,0,0];
    
RANDOM Q0 = lap(eps/4, INPUT[0]);
IF Q0 > TH THEN {
    OUTPUT[0] = 1;
}ELSE {
        
RANDOM Q1 = lap(eps/4, INPUT[1]);
IF Q1 > TH THEN {
    OUTPUT[1] = 1;
}ELSE {
        
RANDOM Q2 = lap(eps/4, INPUT[2]);
IF Q2 > TH THEN {
    OUTPUT[2] = 1;
}
}
}
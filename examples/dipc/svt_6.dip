
INPUTSIZE 3;
RANDOM TH = lap(eps/2, 0);
OUTPUT = [0,0,0];
    
RANDOM Q0 = lap(eps/2, INPUT[0]);
IF Q0 > TH THEN {
    OUTPUT[0] = 1;
}ELSE {
    OUTPUT[0] = 0;
}


RANDOM Q1 = lap(eps/2, INPUT[1]);
IF Q1 > TH THEN {
    OUTPUT[1] = 1;
}ELSE {
    OUTPUT[1] = 0;
}

RANDOM Q2 = lap(eps/2, INPUT[2]);
IF Q2 > TH THEN {
    OUTPUT[2] = 1;
} ELSE {
    OUTPUT[2] = 0;
}
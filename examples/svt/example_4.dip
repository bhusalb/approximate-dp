
INPUTSIZE 4;
RANDOM TH = gauss(eps/4, 1);
OUTPUT = [0];
    
RANDOM Q0 = gauss(eps/2, INPUT[0]);
IF Q0 < TH THEN {
    OUTPUT[0] = 0;
}ELSE {
        
RANDOM Q1 = gauss(eps/2, INPUT[1]);
IF Q1 < TH THEN {
    OUTPUT[0] = 0;
}ELSE {
        
RANDOM Q2 = gauss(eps/2, INPUT[2]);
IF Q2 < TH THEN {
    OUTPUT[0] = 0;
}ELSE {
        
RANDOM Q3 = gauss(eps/2, INPUT[3]);
IF Q3 < TH THEN {
    OUTPUT[0] = 1;
}
}
}
}
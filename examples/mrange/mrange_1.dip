INPUTSIZE 1;

OUTPUT = [0];

RANDOM HIGH = gauss(eps/4, 1);
RANDOM LOW = gauss(eps/4, 0);

RANDOM Q1 = gauss(eps/2, INPUT[0]);

IF Q1 > LOW THEN {
    IF Q1 < HIGH THEN {
        OUTPUT[0] = 1;
    }
} ELSE {
    IF Q1 > LOW THEN {
        IF Q1 > HIGH THEN {
            OUTPUT[0] = 0;
        }
    } ELSE {
        IF Q1 < LOW THEN {
            IF Q1 < HIGH THEN {
                OUTPUT[0] = 0;
            }
        }
    }
}